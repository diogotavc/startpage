{
    "manifest_version": 2,
    "name": "startpage",
    "version": "1.0",
    "description": "Personal startpage with search and quick links",

    "applications": {
        "gecko": {
            "id": "startpage@diogotavc.github.io"
        }
    },

    "developer": {
        "name": "diogotavc",
        "url": "https://github.com/diogotavc/startpage"
    },

    "chrome_url_overrides": {
        "newtab": "index.html"
    },

    "chrome_settings_overrides": {
        "homepage": "index.html"
    },

    "permissions": [
        "activeTab"
    ],

    "web_accessible_resources": [
        "icons/*",
        "styles.css",
        "script.js"
    ]
}